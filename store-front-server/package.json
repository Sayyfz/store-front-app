{
    "name": "storefront_backend",
    "version": "0.1.0",
    "description": "",
    "main": "src/index.ts",
    "scripts": {
        "dev": "nodemon -r dotenv/config src/index.ts",
        "build": "npx tsc",
        "start": "nodemon -r dotenv/config dist/index",
        "migrate-up:prod": "set ENV=production&& db-migrate reset -e production && db-migrate up -e production",
        "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./api/index.js\"",
        "test": "set ENV=test&& db-migrate reset -e test &&  db-migrate up -e test && jasmine-ts -r dotenv/config",
        "tsc": "tsc"
    },
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "express": "^4.17.1",
        "jsonwebtoken": "^9.0.0",
        "pg": "^8.5.1"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.9",
        "@types/jasmine": "^4.3.1",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/pg": "^7.14.7",
        "@types/supertest": "^2.0.12",
        "dotenv": "^16.0.3",
        "jasmine": "^3.6.4",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts": "^0.3.0",
        "nodemon": "^2.0.20",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.9.5"
    }
}
