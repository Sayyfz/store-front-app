{
    "version": 2,
    "builds": [
        {
            "src": "backend/src/**/*.ts",
            "use": "@now/node",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}
